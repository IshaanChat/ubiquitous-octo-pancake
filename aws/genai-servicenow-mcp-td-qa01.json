{
    "containerDefinitions": [
        {
            "name": "genai-servicenow-mcp-td-dev01",
            "image": "123456789012.dkr.ecr.us-west-2.amazonaws.com/genai-servicenow-mcp-td:latest",
            "memory": 4096,
            "cpu": 2048,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "PORT",
                    "value": "8000"
                },
                {
                    "name": "LOG_LEVEL",
                    "value": "info"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/genai-servicenow-mcp-td-dev01",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": [],
            "secrets": [
                {
                    "name": "SERVICE_NOW_MCP_TENANT_ID",
                    "valueFrom": "arn:aws:ssm:us-west-2:123456789012:parameter/genai-servicenow-mcp-td-dev01/SERVICE_NOW_MCP_TENANT_ID"
                },
                {
                    "name": "SERVICE_NOW_MCP_CLIENT_ID",
                    "valueFrom": "arn:aws:ssm:us-west-2:123456789012:parameter/genai-servicenow-mcp-td-dev01/SERVICE_NOW_MCP_CLIENT_ID"
                },
                {
                    "name": "SERVICE_NOW_MCP_CLIENT_SECRET",
                    "valueFrom": "arn:aws:ssm:us-west-2:123456789012:parameter/genai-servicenow-mcp-td-dev01/SERVICE_NOW_MCP_CLIENT_SECRET"
                }
            ]
        }
    ],
    "family": "genai-servicenow-mcp-td-dev01",
    "taskRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc"
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "4096"
    "tags": [
        {
            "key": "Environment",
            "value": "Development"
        },
        {
            "key": "Project",
            "value": "GenAI-ServiceNow-MCP-TD"
        }
    ]
}